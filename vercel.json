{
    "builds": [
      {
        "src": "app/main.py",
        "use": "@vercel/python"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "app/main.py"
      }
    ],
    "functions": {
      "app/api/**/*": {
        "runtime": "python@3.9", 
        "memory": 3008,        
        "maxDuration": 60      
      }
    }
  }